		TCVAR:ARG:反感 -= 20
	ENDIF

	;調教道具による反感軽減補正
	SELECTCASE TRAINNAME:SELECTCOM
	CASE "尿道导管"
		TCVAR:ARG:反感 = DIVIDE(TCVAR:ARG:反感, 90 + ITEM:导管*10)
	;CASE "膣鏡"
	;	TCVAR:ARG:反感 = DIVIDE(TCVAR:ARG:反感, 90 + ITEM:膣鏡*10)
	CASE "眼罩", "拘束", "口枷"
