		TCVAR:ARG:恭順 -= 20 - MIN(ABL:ARG:受虐属性, 5)*5
		TCVAR:ARG:欲情 += MIN(ABL:ARG:受虐属性, 5)*5
		TCVAR:ARG:屈服 += 20
		TCVAR:ARG:習得 -= 10
		TCVAR:ARG:恐怖 += 10
	ENDIF
