	SIF GETBITS(BASE:ARG:育児中, 0, 5) >= 14
		RETURNF 1
CASE "育児中：育児終了"
	RETURNF GETBIT(BASE:ARG:育児中, 6)
CASE "育児中：息子"
	RETURNF GETBIT(BASE:ARG:育児中, 20)
	
ENDSELECT
